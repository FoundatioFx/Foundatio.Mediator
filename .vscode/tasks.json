{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build solution",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$msCompile"
			]
		},
		{
			"label": "run tests",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"test"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"problemMatcher": [
				"$msCompile"
			]
		},
		{
			"label": "run comparison benchmarks",
			"type": "shell",
			"command": "pwsh",
			"args": [
				"-File",
				"${workspaceFolder}/build/Update-BenchmarkDocs.ps1"
			],
			"problemMatcher": []
		},
		{
			"label": "run foundatio benchmarks",
			"type": "shell",
			"command": "pwsh",
			"args": [
				"-File",
				"${workspaceFolder}/build/Update-BenchmarkDocs.ps1",
				"-FoundatioOnly"
			],
			"problemMatcher": []
		}
	]
}